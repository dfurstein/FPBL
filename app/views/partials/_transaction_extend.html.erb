<div class="panel panel-info">
  <% team = nil %>
  <div class="panel-heading"><%= @transaction.processed_at.to_date.to_formatted_s(:long_ordinal) %> &mdash; Contract Extension</div>
  <% Transaction.where(transaction_group_id: @id).each do |extension| %>
    <% if team.nil? %>
      <% team = extension.to_team %>
      <ul class="list-group">
        <li class="list-group-item">
          <h5 class="list-group-item-heading"><%= team.name %></h5>
    <% end %>
          <p class="list-group-item-text">
              <%= extension.player.name %> (<%= extension.extension_year%>)
          </p>
  <% end %>
        </li>
      </ul>
</div>